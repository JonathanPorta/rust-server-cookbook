<%= @install_path %>RustDedicated.exe -batchmode -load `
  +server.hostname "<%= @name %>" `
  +server.maxplayers <%= @maxplayers %> `
  +server.port <%= @port %> `
  +server.identity "<%= @identity %>" `
  +server.seed <%= @seed %> `
  +rcon.port <%= @rcon_port %> `
  +rcon.password "<%= @rcon_password %>" `
  +rcon.ip <%= @rcon_ip %> `
  +server.worldsize <%= @worldsize %> `
  +server.description <%= @server_description %> `
  +server.headerimage <%= @server_headerimage %> `
  +server.url <%= @server_url %> `
  +spawn.max_density <%= @spawn_max_density %> `
  +spawn.max_rate <%= @spawn_max_rate %> `
  +spawn.min_density <%= @spawn_min_density %> `
  +spawn.min_rate <%= @spawn_min_rate %> `
  +craft.instant <%= @craft_instant %> `
  +autoupdate
